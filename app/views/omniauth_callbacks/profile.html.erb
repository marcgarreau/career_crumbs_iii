<div class="container">
  <div class="profile-container">
    <h3>Welcome, <%= @user.first_name %></h3>
    <%= @user.first_name %>
    <%= @user.last_name %>
    <br/>
    <%= @user.headline %>
    <br/>
    <strong>Industry:</strong> <%= @user.industry %>
    <br/>
    <%= image_tag @user.pic_url %>
    <br/>

    <h4>Positions</h4>
    <% @positions.each do |position| %>
      <%= position["company"]["name"] %>
    <% end %>


  </div>
</div>

